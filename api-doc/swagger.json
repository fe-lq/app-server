				"required": [
					"userName",
					"userPhone",
					"userAvatar"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Goods": {
				"properties": {
					"id": {
						"type": "number",
						"format": "double",
						"description": "商品ID"
